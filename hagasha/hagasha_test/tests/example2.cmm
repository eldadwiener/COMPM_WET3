int power (c: int, m:int)
{
    d:int;
    if m==0 then return 1;
    if m==1 then return c;
    d=power(c,m-1);
    return d*c;
}

int power (c: int, m:int);

void main()
{
	write("\nMoodle Test 2 (recursion)\n");
    a:int;
    b:int;
    n:int;

    read(a);
    read(n);
    b = power (a,n);
    write("should be a^n : "); write(b); write("\n");
}
